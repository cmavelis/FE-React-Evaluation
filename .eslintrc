{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["jest"],
  "rules": {
    "indent": ["error", 2],
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}]
  }
}
